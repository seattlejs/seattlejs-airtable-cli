{
  "name": "seattlejs-airtable-cli",
  "version": "1.0.4",
  "description": "data pipeline for producing seattlejs website data",
  "type": "module",
  "main": "./bin/index.js",
  "bin": {
    "seattlejs-airtable-cli": "./bin/index.js"
  },
  "files": [
    "package.json",
    "README.md",
    "bin/"
  ],
  "scripts": {
    "start": "npm run build && node ./bin/index.js",
    "test": "npm run build && mocha test ./bin/test/*.spec.js",
    "build": "tsc --build",
    "lint": "prettier ./**/*.ts --write && eslint ./**/*.ts --fix"
  },
  "dependencies": {
    "airtable": "^0.11.6",
    "jimp": "^0.22.4",
    "netrc-parser": "^3.1.6",
    "prompts": "^2.4.2",
    "slugify": "^1.6.6",
    "untildify": "^5.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/prompts": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^4.2.1",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "lodash": "^4.17.21",
    "mocha": "^10.2.0",
    "prettier": "2.8.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "ignorePatterns": [],
    "parserOptions": {
      "project": "tsconfig.json",
      "sourceType": "module",
      "ecmaVersion": 2022
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seattlejs/seattlejs.com.git"
  },
  "keywords": [
    "seattlejs"
  ]
}
